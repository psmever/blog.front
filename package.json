  "scripts2": {
    "build:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "scripts": {
    "build:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/styles.css",
    "start": "cp config/local.env .env && yarn watch:css && PORT=4000 craco start --reset-cache",
    "start:stage": "cp config/stage.env .env && yarn watch:css && PORT=4000 craco start --reset-cache",
    "start:prod": "cp config/production.env .env && yarn watch:css && PORT=4000 craco start --reset-cache",
    "build": "cp config/stage.env .env && yarn build:css && GENERATE_SOURCEMAP=false craco build",
    "build:prod": "cp config/production.env .env && yarn build:css && GENERATE_SOURCEMAP=false craco build",
    "build:test": "yarn build && serve -s build",
    "test": "craco test",
    "eject": "craco eject",
    "deploy:prod": "cp config/production.env .env && GENERATE_SOURCEMAP=false yarn build:prod && ssh oracle_web \"rm -rf /var/www/site/nicepage/blog-web/*\" && scp -r ./build/* oracle_web:/var/www/site/nicepage/blog-web",
    "deploy:stage": "cp config/stage.env .env && GENERATE_SOURCEMAP=false yarn build:stage && ssh local_server \"rm -rf /var/www/site/blog/front/*\" && scp -r ./build/* local_server:/var/www/site/blog/front",
    "original-start": "react-scripts start",
    "original-build": "react-scripts build",
    "original-test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
